{
  "manifest_version": 2,
  "name": "Tell Consul",
  "version": "1.1.0",
  "description": "Tells consul what's playing",
  "icons": {
      "24": "icons/tellconsul.png"
  },
  "permissions": ["*://localhost/*", "webRequest"],
  "content_scripts": [
      {
        "matches": [
          "*://play.google.com/music/listen*",
          "*://*.netflix.com/*",
          "*://*.youtube.com/*",
          "*://*.shudder.com/play*"
        ],
          "js": ["dist/content.js"]
      }
  ]
}
